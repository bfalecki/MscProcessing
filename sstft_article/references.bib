@article{kodera,
title = {{A new method for the numerical analysis of non-stationary signals}},
journal = {Physics of the Earth and Planetary Interiors},
volume = {12},
number = {2},
pages = {142-150},
year = {1976},
issn = {0031-9201},
doi = {https://doi.org/10.1016/0031-9201(76)90044-3},
url = {https://www.sciencedirect.com/science/article/pii/0031920176900443},
author = {Kunihiko, K. and Claude, {D. V.} and Roger, G.},
abstract = {A modified moving-window method for analyzing non-stationary signals in the frequency-time domain is presented. It is based upon the determination of the position of the center of gravity of the signal power within the given time and frequency resolution of the moving filter. We show how this relates to the use of the phase information which is contained in the conventional power-spectrum analysis and to the concept of instantaneous frequency and group-time delay. The efficiency of the method to discriminate between various non-stationary signals is illustrated by numerical examples, both artificial and natural.}
}



